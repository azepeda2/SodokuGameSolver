
public class Choice {

	private int col;
	private int row;
	private int value;
	
	public Choice(int row, int col, int value) {
		this.col = col;
		this.row = row;
		this.value = value;
	}
	
	public int getCol() {
		return this.col;
	}
	
	public int getRow() {
		return this.row;
	}
	
	public int getValue() {
		return this.value;
	}
	
	public void setCol(int col) {
		this.col= col;
	}
	
	public void setRow(int row) {
		this.row= row;
	}
	
	public void setValue(int value) {
		this.value= value;
	}

}